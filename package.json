    "tsds-lib": "^1.20.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "module-link-unlink": "^1.0.11",
    "node-version-use": "^2.1.6",
    "os-shim": "^0.1.3",
    "queue-cb": "^1.6.3",
    "resolve": "^1.22.11",
    "short-hash": "^1.0.0",
    "ts-dev-stack": "^1.21.3",
    "tsds-config": "^0.2.1",
    "tsds-lib-test": "^1.19.5"
  },
  "peerDependencies": {
    "tsds-config": ">=0.0.1"
